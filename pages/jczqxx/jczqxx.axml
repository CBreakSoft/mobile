<view>
  <tabs
    tabs="{{tabs}}"
    onTabClick="handleTabClick"
    activeTab="{{activeTab}}"
  >
    <block a:for="{{tabs}}">
      <tab-content key="{{index}}">
  
      </tab-content>
    </block>
  </tabs>
</view>

<view style={{display:activeTab===0?"block":"none"}}>
          <scroll-view style="height: 100vh;" scroll-y>
            <list>
              <block a:for="{{items}}">
                <list-item >
                  {{item.ywfsrq}}
                  <view class="am-list-brief">{{item.ywzy}}</view>
                  <view slot="extra">
                    {{item.yue}}
                  </view>
              </list-item>
              </block>
            </list>
          </scroll-view>
        </view>

